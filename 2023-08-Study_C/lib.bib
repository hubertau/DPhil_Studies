  author = {Rocha, Anderson and Scheirer, Walter J. and Forstall, Christopher W. and Cavalcante, Thiago and Theophilo, Antonio and Shen, Bingyu and Carvalho, Ariadne R.B. and Stamatatos, Efstathios},
