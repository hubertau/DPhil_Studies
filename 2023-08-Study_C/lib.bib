@article{Beguerisse-Diaz2014,
  title = {Interest Communities and Flow Roles in Directed Networks: {{The Twitter}} Network of the {{UK}} Riots},
  author = {{Beguerisse-D{\'i}az}, Mariano and {Gardu{\~n}o-Hern{\'a}ndez}, Guillermo and Vangelov, Borislav and Yaliraki, Sophia N. and Barahona, Mauricio},
