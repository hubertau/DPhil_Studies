@article{romanowskaStudyCenturiesLongReliance2022,
  title = {A {{Study}} of the {{Centuries-Long Reliance}} on {{Local Ceramics}} in {{Jerash Through Full Quantification}} and {{Simulation}}},
  author = {Romanowska, Iza and Brughmans, Tom and Bes, Philip and Carrignon, Simon and Egelund, Line and Lichtenberger, Achim and Raja, Rubina},
  year = {2022},
  month = mar,
  journal = {Journal of Archaeological Method and Theory},
  volume = {29},
